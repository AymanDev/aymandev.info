{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "esnext",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "lib": ["esnext", "dom"]
    },
    "include": ["./src/**/*"],
    "exclude": [
        "./node_modules"
    ]
}
